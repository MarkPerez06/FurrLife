@model IEnumerable<Menu>

<a href="javascript: void(0);" onclick="LoadProductsAll('MenuProducts','_ProductsAll')">All Items</a>
@if (ViewBag.HasOrderProducts == true)
{
    <a href="javascript: void(0);" onclick="LoadProductsAll('MenuProducts','_ProductsBestSeller')"><i class="mdi mdi-chevron-right me-1"></i> Best Seller</a>
}

@foreach (var item in Model)
{
    <a href="javascript: void(0);" onclick="LoadMenu('@item.Id','@item.Name','@item.Description')" ><i class="mdi mdi-chevron-right me-1"></i> @item.Name</a>
}